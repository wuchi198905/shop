<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.shop.management.mapper.CarouselMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.shop.management.bean.Carousel">
        <id column="carousel_id" property="carouselId" />
        <result column="path" property="path" />
        <result column="type" property="type" />
        <result column="jump_link" property="jumpLink" />
        <result column="jumped_text" property="jumpedText" />
        <result column="order" property="order" />
        <result column="whether_show" property="whetherShow" />
        <result column="creation_time" property="creationTime" />
        <result column="update_time" property="updateTime" />
        <result column="sts" property="sts" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        carousel_id AS carouselId, path, type, jump_link AS jumpLink, jumped_text AS jumpedText, order, whether_show AS whetherShow, creation_time AS creationTime, update_time AS updateTime, sts
    </sql>

</mapper>
